{
    "kind": "List",
    "apiVersion": "v1",
    "items": [
        {
            "kind": "Deployment",
	    "apiVersion": "extensions/v1beta1",
            "metadata": {
                "name": "memcached1",
                "labels": {
                    "name": "memcached1",
		    "service" : "seldon"
                }
            },
            "spec": {
                "replicas": 1,
                "selector": {
                    "matchLabels": {
                        "name": "memcached1"
                    }
                },
                "template": {
                    "metadata": {
                        "name": "memcached1",
                        "creationTimestamp": null,
                        "labels": {
                            "name": "memcached1"
                        }
                    },
                    "spec": {
                        "containers": [
                            {
                                "name": "memcached1",
                                "image": "seldonio/memcached:0.1",
				"command": [
				    "memcached",
				    "-m","512"
				],
				"resources": {
					%MEMCACHE_RESOURCES%
				},
                                "ports": [
                                    {
                                        "containerPort": 11211,
                                        "protocol": "TCP"
                                    }
                                ]
                            }
                        ]
                    }
                }
            }
        },
        {
            "kind": "Service",
            "apiVersion": "v1",
            "metadata": {
                "name": "memcached1",
                "labels": {
                    "app": "memcache",
                    "name": "memcached1",
		    "service" : "seldon"
                }
            },
            "spec": {
                "ports": [
                    {
                        "protocol": "TCP",
                        "port": 11211,
                        "targetPort": 11211
                    }
                ],
                "selector": {
                    "name": "memcached1"
                },
                "type": "ClusterIP",
                "sessionAffinity": "None"
            }
        },
        {
            "kind": "Deployment",
	    "apiVersion": "extensions/v1beta1",
            "metadata": {
                "name": "memcached2",
                "labels": {
                    "name": "memcached2",
		    "service" : "seldon"
                }
            },
            "spec": {
                "replicas": 1,
                "selector": {
                    "matchLabels": {
                        "name": "memcached2"
                    }
                },
                "template": {
                    "metadata": {
                        "name": "memcached2",
                        "labels": {
                            "name": "memcached2"
                        }
                    },
                    "spec": {
                        "containers": [
                            {
                                "name": "memcached2",
                                "image": "seldonio/memcached:0.1",
				"command": [
				    "memcached",
				    "-m","512"
				],
				"resources": {
					%MEMCACHE_RESOURCES%
				},
                                "ports": [
                                    {
                                        "containerPort": 11211,
                                        "protocol": "TCP"
                                    }
                                ]
                            }
                        ]
                    }
                }
            }
        },
        {
            "kind": "Service",
            "apiVersion": "v1",
            "metadata": {
                "name": "memcached2",
                "labels": {
                    "app": "memcache",
                    "name": "memcached2",
		    "service" : "seldon"
                }
            },
            "spec": {
                "ports": [
                    {
                        "protocol": "TCP",
                        "port": 11211,
                        "targetPort": 11211
                    }
                ],
                "selector": {
                    "name": "memcached2"
                },
                "type": "ClusterIP",
                "sessionAffinity": "None"
            },
            "status": {
                "loadBalancer": {}
            }
        }
    ]
}
